<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/t1"
        android:layout_width="30sp"
        android:layout_height="40sp"
        android:gravity="center_horizontal"

        android:textColor="@android:color/black" />

    <View
        android:layout_width="2dip"
        android:layout_height="40sp"
        android:layout_toRightOf="@id/t1"
        android:background="#ffffff" />

    <TextView
        android:id="@+id/t2"
        android:layout_width="70sp"

        android:layout_height="40sp"

        android:layout_toRightOf="@id/t1"
        android:gravity="center_horizontal"
        android:textColor="@android:color/black" />

    <View
        android:layout_width="2dip"
        android:layout_height="40sp"
        android:layout_toRightOf="@id/t2"
        android:background="#ffffff" />

    <TextView
        android:id="@+id/t3"
        android:layout_width="50sp"

        android:layout_height="40sp"

        android:layout_toRightOf="@id/t2"
        android:gravity="center_horizontal"
        android:textColor="@android:color/black" />

    <View
        android:layout_width="2dip"
        android:layout_height="40sp"
        android:layout_toRightOf="@id/t3"
        android:background="#ffffff" />

    <TextView
        android:id="@+id/t4"
        android:layout_width="60sp"


        android:layout_height="40sp"
        android:layout_toRightOf="@id/t3"
        android:gravity="center_horizontal"
        android:textColor="@android:color/black" />

    <View
        android:layout_width="2dip"
        android:layout_height="40sp"
        android:layout_toRightOf="@id/t4"
        android:background="#ffffff" />

    <TextView
        android:id="@+id/t5"
        android:layout_width="40sp"


        android:layout_height="40sp"
        android:layout_toRightOf="@id/t4"
        android:gravity="center_horizontal"
        android:textColor="@android:color/black" />

    <View
        android:layout_width="2dip"
        android:layout_height="40sp"
        android:layout_toRightOf="@id/t5"
        android:background="#ffffff" />

    <TextView
        android:id="@+id/t6"
        android:layout_width="50sp"


        android:layout_height="40sp"
        android:layout_toRightOf="@id/t5"
        android:gravity="center_horizontal"
        android:textColor="@android:color/black" />

    <View
        android:layout_width="2dip"
        android:layout_height="40sp"
        android:layout_toRightOf="@id/t6"
        android:background="#ffffff" />

    <TextView
        android:id="@+id/t7"
        android:layout_width="50sp"


        android:layout_height="40sp"
        android:layout_toRightOf="@id/t6"
        android:gravity="center_horizontal"
        android:textColor="@android:color/black" />

    <View
        android:layout_width="2dip"
        android:layout_height="40sp"
        android:layout_toRightOf="@id/t7"
        android:background="#ffffff" />

    <TextView
        android:id="@+id/t8"
        android:layout_width="70sp"


        android:layout_height="40sp"
        android:layout_toRightOf="@id/t7"
        android:gravity="center_horizontal"
        android:textColor="@android:color/black" />

    <View
        android:layout_width="2dip"
        android:layout_height="40sp"
        android:layout_toRightOf="@id/t8"
        android:background="#ffffff" />

    <TextView
        android:id="@+id/t9"
        android:layout_width="70sp"


        android:layout_height="40sp"
        android:layout_toRightOf="@id/t8"
        android:gravity="center_horizontal"
        android:textColor="@android:color/black" />

    <View
        android:id="@+id/view1"
        android:layout_width="2dip"
        android:layout_height="40sp"
        android:layout_toRightOf="@id/t9"
        android:background="#ffffff" />

    <TextView
        android:id="@+id/t10"
        android:layout_width="70sp"


        android:layout_height="40sp"
        android:layout_toRightOf="@id/t9"
        android:gravity="center_horizontal"
        android:textColor="@android:color/black" />

    <View
        android:layout_width="wrap_content"
        android:layout_height="2sp"
        android:layout_below="@id/view1"
        android:background="#ffffff" />


</RelativeLayout>
